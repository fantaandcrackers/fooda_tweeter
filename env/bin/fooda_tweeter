#!/Users/arthurshir/Documents/Job_Search/Interviews/Interviews/fooda/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'fooda-tweeter==0.1','console_scripts','fooda_tweeter'
__requires__ = 'fooda-tweeter==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fooda-tweeter==0.1', 'console_scripts', 'fooda_tweeter')()
    )
